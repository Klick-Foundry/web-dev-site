---
import { cn } from "@utils/utils";
import Footer269 from "./footer-269.astro";
import Footer269Li from "./footer-269-li.astro";
import Footer269Contact from "./footer-269-contact.astro";
import Footer269Social from "./footer-269-social.astro";
import client from "@data/client.json"
import Footer1390Link from "./footer-1390-link.astro";

interface Props {}

const {} = Astro.props;

// Styles
---

<Footer269
  logoSrc="/src/assets/images/web-dev/logo-basic.webp"
  text="Web design and development experts in the Phoenix area"
>
  <Footer269Li href="/" text="Home" slot="li" />
  <Footer269Li href="/about" text="About" slot="li" />
  <Footer269Li href="/services" text="Services" slot="li" />
  <!-- <Footer269Li href="" text="Portfolio" slot="li" /> -->
  <Footer269Li href="/contact" text="Contact" slot="li" />
  <Footer269Contact
    slot="contact"
    isPhone
    phone={client.phoneFormatted}
    phoneHref={client.phoneForTel}
  />
  <Footer269Contact slot="contact" isEmail email={client.email} />
